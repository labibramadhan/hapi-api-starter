{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "0.1.0",
  "command": "npm",
  "isShellCommand": true,
  "showOutput": "always",
  "suppressTaskName": true,
  "tasks": [{
    "taskName": "lint",
    "args": ["run", "lint"]
  }, {
    "taskName": "test",
    "args": ["test"]
  }, {
    "taskName": "test-core",
    "args": ["run", "test-core"]
  }, {
    "taskName": "test-debug",
    "args": ["run", "test-debug"]
  }, {
    "taskName": "test-core-debug",
    "args": ["run", "test-core-debug"]
  }]
}
