{
  "user": {
    "database": "main",
    "public": true,
    "singular": "user",
    "plural": "users",
    "ownerFields": ["id"],
    "methods": {
      "findAll": {
        "authenticate": true,
        "authentications": {
          "include": ["role", "session"]
        }
      },
      "count": {
        "authenticate": true
      },
      "create": {
        "authenticate": true
      },
      "update": {
        "authenticate": true
      },
      "findOne": {
        "authenticate": true
      },
      "findById": {
        "authenticate": true
      },
      "roleFindAll": {
        "authenticate": true
      },
      "roleCount": {
        "authenticate": true
      },
      "sessionFindAll": {
        "authenticate": true
      },
      "sessionCount": {
        "authenticate": true
      }
    },
    "relationships": [{
      "type": "hasMany",
      "model": "session"
    }, {
      "type": "belongsToMany",
      "model": "role",
      "through": "userRole",
      "foreignKey": "userId"
    }]
  }
}
