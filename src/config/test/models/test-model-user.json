{
  "user": {
    "database": "main",
    "plural": "users",
    "ownerFields": ["id"],
    "schema": {
      "username": {
        "type": "STRING",
        "unique": true,
        "allowNull": false,
        "validate": {
          "len": [4, 15]
        }
      },
      "email": {
        "type": "STRING",
        "unique": true,
        "allowNull": false,
        "validate": {
          "isEmail": true,
          "notEmpty": true,
          "len": [6, 255]
        }
      },
      "password": {
        "type": "STRING",
        "allowNull": false,
        "validate": {
          "notEmpty": true
        }
      }
    },
    "options": {
      "schema": "core",
      "freezeTableName": true,
      "indexes": [{
        "unique": true,
        "fields": ["email", "username"]
      }]
    },
    "methods": {
      "findAll": {},
      "count": {},
      "create": {},
      "update": {},
      "findOne": {},
      "findById": {},
      "associations": {
        "roles": {
          "findAll": {},
          "count": {},
          "add": {},
          "addMultiple": {},
          "remove": {},
          "removeMultiple": {}
        },
        "sessions": {
          "findAll": {},
          "count": {}
        }
      }
    },
    "relationships": [{
      "type": "hasMany",
      "model": "session"
    }, {
      "type": "belongsToMany",
      "model": "role",
      "through": "userRole",
      "foreignKey": "userId"
    }]
  }
}
