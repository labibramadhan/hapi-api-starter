{
  "user": {
    "database": "main",
    "public": true,
    "plural": "users",
    "ownerFields": ["id"],
    "methods": {
      "findAll": {
        "authenticate": true,
        "authentications": {
          "include": ["role", "session"]
        }
      },
      "count": {
        "authenticate": true
      },
      "create": {
        "authenticate": true
      },
      "update": {
        "authenticate": true
      },
      "findOne": {
        "authenticate": true
      },
      "findById": {
        "authenticate": true
      },
      "associations": {
        "roles": {
          "findAll": {
            "authenticate": true
          },
          "count": {
            "authenticate": true
          }
        },
        "sessions": {
          "findAll": {
            "authenticate": true
          },
          "count": {
            "authenticate": true
          }
        }
      }
    },
    "relationships": [{
      "type": "hasMany",
      "model": "session"
    }, {
      "type": "belongsToMany",
      "model": "role",
      "through": "userRole",
      "foreignKey": "userId"
    }]
  }
}
